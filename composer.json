{
    "name": "jhancock/yii2-version-assetmgr",
    "description": "Yii2 asset manager that hashes based on application version instead of mtime. Useful for load balanced sites.",
    "homepage": "https://github.com/jasonhancock/yii2-version-assetmgr",
    "type": "yii2-extension",
    "keywords": ["yii2", "assetmanager"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Hancock",
            "email": "jason@jasonhancock.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jasonhancock/yii2-version-assetmgr/issues",
        "source": "https://github.com/jasonhancock/yii2-version-assetmgr"
    },
    "autoload": {
        "psr-4" : {
            "jhancock\\VersionAssetMgr\\" : "src/"
        }
    },
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    }
}
